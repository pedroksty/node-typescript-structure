{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "space-before-function-paren": "off",
        "no-unused-vars": "off",
        "camelcase": "off",
        "no-tabs":"off",
        "no-mixed-spaces-and-tabs":"off",
        "no-useless-constructor":"off"
    }
}